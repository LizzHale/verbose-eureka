<div class="col-md-10 col-md-offset-0 col-xs-10 col-xs-offset-1">
  <div class="gallery">
    <div class="grid">
      <ul id="lightgallery">
        <% @photos.each do |photo| %>
          <li class="grid-item" data-src="<%= photo.large_location %>" data-download-url="false">
            <img class="img-responsive" src="<%= photo.thumbnail_location %>">
          </li>
        <% end %>
      </ul>
    </div>
  </div>
</div>
